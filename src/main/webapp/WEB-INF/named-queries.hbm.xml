<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"https://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	
	

	
	<query name="CustomerDetails.findByCustId">
		<![CDATA[FROM CustomerDetails c WHERE c.custid = :custid]]>
	</query>
	
	
	
	
</hibernate-mapping>